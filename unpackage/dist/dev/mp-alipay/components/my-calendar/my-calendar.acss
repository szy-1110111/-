.data-v-afe5605e:root {
  --white: #ffffff;
  --grey: #959595;
  --primary-2: #409cff;
  --red: #f7220e;
  --yellow: #f7a90d;
  --primary-1: #eef7f2;
  --greyLight-2: #c8d0e7;
  --primary: #57c3c2;
}
.Cbody.data-v-afe5605e {
  margin: 20rpx;
  width: 95vw;
  height: 55vh;
  border-radius: 20rpx;
  display: flex;
  box-sizing: border-box;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.Cbody .Cheader.data-v-afe5605e {
  height: 90rpx;
  padding-top: 10rpx;
  box-sizing: border-box;
  font-size: 38rpx;
  width: 100%;
  text-align: center;
  background-color: var(--white);
}
.Cbody .Cweek.data-v-afe5605e {
  height: 80rpx;
  box-sizing: border-box;
  width: 100%;
  display: grid;
  border-top: #d9d8d8 1rpx dashed;
  background-color: var(--white);
  grid-template-columns: repeat(7, 1fr);
  overflow: hidden;
  border-radius: 0 0 20rpx 20rpx;
}
.Cbody .Cweek .weekText.data-v-afe5605e {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.Cbody .Cmain.data-v-afe5605e {
  transition: all 0.5s ease;
  flex: 1;
  width: 100%;
  margin-top: 10rpx;
  padding: 14rpx 0;
  box-sizing: content-box;
  background-color: var(--white);
  border-radius: 20rpx 20rpx 0 0;
  overflow: hidden;
}
.Cbody .Cmain .dateTable.data-v-afe5605e {
  display: grid;
  padding: 0 10rpx;
  grid-template-columns: repeat(7, 1fr);
  grid-template-rows: repeat(6, 1fr);
  gap: 10rpx;
  height: 100%;
}
.Cbody .Cmain .dateTable .dateText.data-v-afe5605e {
  transition: all 0.4s ease;
  height: 100%;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  box-sizing: border-box;
}
.Cbody .Cmain .dateTable .dateText .mark.data-v-afe5605e {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
}
.isToday.data-v-afe5605e {
  background-color: var(--primary-1);
  border: var(--primary) solid 2rpx;
}
.otherMonth.data-v-afe5605e {
  color: var(--grey);
}
.patchSucces.data-v-afe5605e {
  height: 18rpx !important;
  width: 18rpx !important;
}
.isSelect.data-v-afe5605e {
  background-color: var(--primary) !important;
  color: var(--white) !important;
}
